{
    "version": 2,
    "framework": null,
    "outputDirectory": ".",
    "public": true,
    "functions": {
        "api/index.php": {
            "runtime": "vercel-php@0.6.0"
        }
    },
    "routes": [
        {
            "src": "/(assets|build|css|js|img|fonts|favicon.ico|favicon.png)/(.*)",
            "dest": "public/$1/$2"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ]
}